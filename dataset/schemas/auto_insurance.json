{
  "type": "index_parallel",
  "dataSchema": {
    "dataSource": "ping_logs",
    "granularitySpec": {
      "type": "uniform",
      "segmentGranularity": "DAY",
      "queryGranularity": "HOUR",
      "rollup": true
    },
    "parser": {
      "type": "string",
      "parseSpec": {
        "format": "json",
        "timestampSpec": {
          "column": "timestamp",
          "format": "iso"
        },
        "dimensionsSpec": {
          "dimensions": [
            {
              "name": "age",
              "type": "long"
            },
            "current_insurance_co",
            "currently_insured",
            "gender",
            "has_tcpa_consent_text",
            "landing_page_domain",
            {
              "name": "lead_age",
              "type": "float"
            },
            "lead_id_code",
            "marital_status",
            {
              "name": "num_drivers",
              "type": "long"
            },
            {
              "name": "num_vehicles",
              "type": "long"
            },
            {
              "name": "num_violations",
              "type": "long"
            },
            {
              "name": "months_at_employer",
              "type": "long"
            },
            {
              "name": "months_at_residence",
              "type": "long"
            },
            "occupation",
            "ping_response",
            {
              "name": "ping_price",
              "type": "float"
            },
            "residence_type",
            "source_id",
            "state",
            "tcpa_compliant",
            "trusted_form_cert",
            "vehicle_make",
            "vehicle_ownership",
            "vehicle_year",
            "vendor_campaign_id",
            "zip3"
          ]
        }
      }
    },
    "metricsSpec": [
      {
        "name": "count",
        "type": "count"
      }
    ]
  },
  "ioConfig": {
    "type": "index_parallel",
    "firehose": {
      "type": "local",
      "baseDir": "/dataset/pinglogs/2018-10-14-16",
      "filter": "auto_insurance.txt"
    }
  },
  "tuningConfig": {
    "type": "index_parallel"
  }
}
